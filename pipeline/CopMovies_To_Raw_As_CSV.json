{
	"name": "CopMovies_To_Raw_As_CSV",
	"properties": {
		"activities": [
			{
				"name": "Copy_3me",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "movies"
					},
					{
						"name": "Destination",
						"value": "raw/Movies/Movies.csv"
					}
				],
				"typeProperties": {
					"source": {
						"type": "CosmosDbSqlApiSource",
						"query": "select c.id, actors.name, c.title, c.genre, c.rating, c.runtime, c.releaseYear, c.availabilityDate, c.streamingAvailabilityDate\r\n\r\nfrom c\r\n\r\njoin actors in c.actors",
						"preferredRegions": []
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['id']"
								},
								"sink": {
									"name": "id"
								}
							},
							{
								"source": {
									"path": "$['name']"
								},
								"sink": {
									"name": "name"
								}
							},
							{
								"source": {
									"path": "$['title']"
								},
								"sink": {
									"name": "title"
								}
							},
							{
								"source": {
									"path": "$['genre']"
								},
								"sink": {
									"name": "genre"
								}
							},
							{
								"source": {
									"path": "$['rating']"
								},
								"sink": {
									"name": "rating"
								}
							},
							{
								"source": {
									"path": "$['runtime']"
								},
								"sink": {
									"name": "runtime"
								}
							},
							{
								"source": {
									"path": "$['releaseYear']"
								},
								"sink": {
									"name": "releaseYear"
								}
							},
							{
								"source": {
									"path": "$['availabilityDate']"
								},
								"sink": {
									"name": "availabilityDate"
								}
							},
							{
								"source": {
									"path": "$['streamingAvailabilityDate']"
								},
								"sink": {
									"name": "streamingAvailabilityDate"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "SourceDataset_3me",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DestinationDataset_3me",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}